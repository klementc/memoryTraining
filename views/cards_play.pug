extends challenge

block data
  div(id="memZone" class="table-responsive")
    table(class="table table-striped table-hover table-bordered")
      thead
      tbody
        - var i = 0
        each c in cards
          if i % group_by == 0
            tr
          td
            img(src="/images/cards/"+c width="100" height="150") 
          - i++