extends challenge

block data
  div(id="memZone" class="table-responsive")
    table(class="table table-striped table-hover table-bordered")
      thead
        tr
          th(scope='col') # Row
          th(scope='col') Words
      tbody
        each n, index in word_list
          tr
            th(scope='row') #{index}
            td
              each word in n
                | #{word} 